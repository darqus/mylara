{
  "name": "@amzn/mylara",
  "version": "0.1.45",
  "description": "Косметика по уходу за лицом",
  "productName": "mylara-landing",
  "author": "darqus",
  "license": "MIT",
  "type": "module",
  "private": false,
  "scripts": {
    "dev": "quasar dev",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "build": "quasar build",
    "format": "bash format.sh",
    "format:quick": "bash format-quick.sh",
    "format:validate": "bash validate-config.sh",
    "postinstall": "quasar prepare",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "inspect": "quasar inspect --depth",
    "update-env": "node update-env.js",
    "deploy": "npm run update-env && npm run build && cp CNAME dist/spa/ && cp .nojekyll dist/spa/ && cp _config.yml dist/spa/ && gh-pages -d dist/spa",
    "prettier:check": "prettier --check \"**/*.{ts,mts,js,mjs,cjs,vue,scss,css,md,json}\" --config .prettierrc.json",
    "prettier:write": "prettier --write \"**/*.{ts,mts,js,mjs,cjs,vue,scss,css,md,json}\" --config .prettierrc.json",
    "stylelint": "stylelint \"**/*.{scss,css}\" --config stylelint.config.mjs",
    "stylelint:fix": "stylelint --fix \"**/*.{scss,css}\" --config stylelint.config.mjs",
    "lint:all": "npm run lint && npm run stylelint"
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "firebase": "^11.10.0",
    "quasar": "^2.16.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@quasar/app-vite": "^2.2.1",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.2.3",
    "@types/node": "^24.0.13",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^10.3.0",
    "gh-pages": "^6.1.1",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "stylelint": "^16.21.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-checker": "^0.10.0",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.1"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4"
  }
}