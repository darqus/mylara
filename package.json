    "stylelint-order": "^7.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.1",
    "vite-plugin-checker": "^0.9.3",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.1"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
